{
  "name": "frontend",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite & npm run watch:css",
    "build": "tsc && vite build && npm run build:css",
    "build:css": "npx tailwindcss -i ./src/styles/index.css -o ./src/styles/compiled.css ",
    "watch:css": "npx tailwindcss -i ./src/styles/index.css -o ./src/styles/compiled.css --watch",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "start": "react-scripts start",
    "test": "react-scripts test",
    "prestart": "npm run tailwind",
    "prebuild": "npm run tailwind",
    "tailwind": "postcss src/index.css -o src/tailwind.css"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.1.0",
    "autoprefixer": "^10.4.14",
    "axios": "^1.4.0",
    "flowbite": "^1.6.5",
    "postcss": "^8.4.24",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.44.3",
    "react-router-dom": "^6.11.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@typescript-eslint/eslint-plugin": "^5.59.7",
    "@typescript-eslint/parser": "^5.59.7",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.41.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "prettier": "^2.8.8",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
